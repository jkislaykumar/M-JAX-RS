<!DOCTYPE web-app PUBLIC '-//Sun Microsystems, Inc.//DTD Web 
     Application 2.3//EN' 'http://java.sun.com/dtd/web-app_2_3.dtd'>

<web-app>
	<servlet>
		<servlet-name>SimpleServlet</servlet-name>
		<servlet-class>simple.MyServlet</servlet-class>
		<init-param>
			<param-name>SimpleServletInitParam</param-name>
			<param-value>SimpleServletInitParamValue</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>SimpleServlet</servlet-name>
		<url-pattern>/SimpleServlet</url-pattern>
	</servlet-mapping>
	<context-param>
		<param-name>MyContextParam</param-name>
		<param-value>MyContextParamValue</param-value>
	</context-param>
</web-app>